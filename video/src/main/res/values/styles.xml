<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppBaseTheme" parent="@style/AppTheme"></style>

    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowTranslucentNavigation" tools:targetApi="kitkat">false</item>
    </style>
    <style name="CustomCheckboxTheme_Licaishi" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_style</item>
    </style>

    <style name="videoplay_product_txt_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/dimens_sp_15</item>
        <item name="android:textColor">@color/c_mid_black</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="video_bottom_line">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">3dp</item>
        <item name="android:layout_marginTop">@dimen/ui_2_dip</item>
        <item name="android:layout_marginBottom">@dimen/ui_5_dip</item>
        <item name="android:src">@color/app_golden</item>
    </style>
</resources>