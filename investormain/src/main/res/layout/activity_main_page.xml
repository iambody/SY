<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cgbsoft.privatefund.mvp.ui.home.MainPageActivity">

    <com.cgbsoft.lib.base.webview.BaseWebview
        android:id="@+id/webView"
        app:init="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible"/>

    <FrameLayout
        android:id="@+id/fl_main_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white" />

    <!--<include layout="@layout/acitivity_divide_vertical"-->
        <!--android:layout_marginBottom="49dp"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="1dp"-->
        <!--android:layout_gravity="bottom"-->
        <!--/>-->

    <com.cgbsoft.privatefund.widget.navigation.BottomNavigationBar
        tools:context="com.cgbsoft.privatefund.mvp.ui.home.MainPageActivity"
        android:id="@+id/bottomNavigationBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        app:layout_behavior="com.cgbsoft.privatefund.widget.navigation.ScrollingBehavior" />

    <include
        android:id="@+id/cmain_live_dialog"
        layout="@layout/cmain_live_dialog"
        android:layout_gravity="bottom"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        android:layout_marginBottom="15dp"
        android:visibility="gone"
        />
</android.support.design.widget.CoordinatorLayout>
